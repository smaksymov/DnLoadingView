                android:color="@color/seed_border_color" />

            <solid android:color="@color/seed_color" />

            <corners android:topLeftRadius="@dimen/button_radius" android:topRightRadius="@dimen/button_radius"
                android:bottomLeftRadius="@dimen/button_radius" android:bottomRightRadius="@dimen/button_radius"/>
        </shape>
    </item>

</layer-list>